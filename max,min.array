#include <iostream>
using namespace std;

void getMinMax(int arr[], int n)
{
    int max=arr[0],min=arr[0];
    for(int i=1;i<n;i++)
    {
        if(max<arr[i])
            max=arr[i];
        if(min>arr[i])
            min=arr[i];
    }
    cout<<"max"<<max<<endl;
    cout<<"min"<<min<<endl;
}

int main()
{
    int arr[] = {1,2,3,4,6,8};
    int n=6;
    getMinMax(arr,n);
    return 0;
}
