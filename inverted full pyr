#include<iostream>
using namespace std;
int main()
{
    int i, space, j,n,k;
    cout<<"enter no. of rows";
    cin>>n;
    for(i=n;i>=1;i--)
    {
		for(j=1;j<=n-i;j++)
		{
            printf(" ");
		}
		for(k=1;k<=2*i-1;k++)
		{
		    printf("*");
		}
		cout<<endl;
    }
		    
    
    return 0;
}
