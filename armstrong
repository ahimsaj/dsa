#include<iostream>
#include<stdio.h>
using namespace std;

int main()
{
   int i,n,sum=0,temp,r;
   cout<<"enter value of n";
   cin>>n;
   temp=n;
   while(n>0)
   {
       r=n%10;
       sum=sum+(r*r*r);
       n=n/10;
   }
   if(temp==sum)
   {
       cout<<"armstrong";
   }
   else
   {
       cout<<"not";
   }
    return 0;
}
